{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Auth": {
    "Basic": {
      "Username": "admin",
      "Password": "secret123",
      "UserId": "11111111-1111-1111-1111-111111111111"
    }
  },
  "Api": {
    "BaseAddress": "https://localhost:7031"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Blinked.App.Handlers": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Blinked.App.Handlers": "Debug",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "C:/Logs/blinked.dev.log",
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ]
  }
}